class Solution {
    public int smallestRepunitDivByK(int k) {
        if(k%2 == 0)return -1;
        else if(k%5 == 0)return -1;
        int arr= 1;
        int ans = 1;
        
        while(ans<=k){
            if(arr%k == 0)return ans;
            arr= arr%k;
            arr*=10;
            arr++;
            ans++;
        }
        
        
        return -1;


    }
}
