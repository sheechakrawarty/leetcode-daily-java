class Solution {
    public int numberOfPaths(int[][] grid, int k) {
        return dfs(k,grid,0,0,0,0);
    }

    int dfs(int k, int grid[][], int sum, int i, int j, int ans){
        if(i>= grid.length || j>= grid[i].length)return ans;
        sum += grid[i][j];
        if(i== grid.length-1 && j == grid[i].length-1){
            if(sum%k == 0)return ans+1;
            else return ans;
        }
        return dfs(k,grid,sum,i+1,j,ans)
            + dfs(k,grid,sum,i,j+1,ans);
    }
}
